{
  "expo": {
    "name": "AirCnC",
    "slug": "mobile",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "android": {
      "package": "com.guisantos.aircnc"
    }
  }
}
